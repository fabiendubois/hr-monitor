<workout_file>
    <author>Jules</author>
    <name>Ramp Test ZWO</name>
    <description>A test workout with ramps and intervals.</description>
    <workout>
        <Warmup Duration="600" PowerLow="0.25" PowerHigh="0.75"/>
        <SteadyState Duration="300" Power="0.75"/>
        <IntervalsT Repeat="3" OnDuration="60" OffDuration="60" OnPower="1.0" OffPower="0.5"/>
        <Ramp Duration="300" PowerLow="0.75" PowerHigh="0.25"/>
        <CoolDown Duration="300" PowerLow="0.25" PowerHigh="0.25"/>
    </workout>
</workout_file>
